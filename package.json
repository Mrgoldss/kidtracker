{
  "name": "kid-tracker",
  "version": "0.0.1",
  "description": "Kid Smart Watch Tracker Application",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:index": "browserify src/main/js/app.js > src/main/resources/static/js/index.js",
    "clean": "npm-run-all clean:*",
    "clean:index": "rimraf src/main/resources/static/index.js",
    "start": "npm run clean && npm run build"
  },
  "dependencies": {
    "moment": "^2.26.0",
    "jquery-ui": "^1.12.1"
  },
  "devDependencies": {
    "browserify": "^14.3.0",
    "copyfiles": "^1.2.0",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1"
  }
}
